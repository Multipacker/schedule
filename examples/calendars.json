[
    {
        "name": "example",
        "urls": [
            "https://cloud.timeedit.net/chalmers/web/public/ri6u7YQQZ9QZnZQ5be44Q8Z35610y.ics"
        ],
        "rules": [
            // Only include these two courses and anything that isn't a course.
            {
                "include": {
                    "Kurs kod": [ "LMA017", "SSY081", "" ]
                }
            },
            // Set the location for all events.
            {
                "include": "*",
                "location": "$[Lokalnamn]"
            },
            // Set summary and description for things that aren't courses.
            {
                "include": { "Kurs kod": "" },
                "summary":     "$[Titel]",
                "description": "$[Kommentar]"
                
            },
            // Set formatting for courses.
            {
                "include": { "Kurs kod": "*" },
                "summary": "$[Titel,Activity>: ]"
            },
            {
                "include": { "Kurs kod": "LMA017" },
                "summary": "$[summary]LMA017"
            },
            {
                "include": { "Kurs kod": "SSY081" },
                "summary": "$[summary]SSY081"
            }
        ]
    }
]
